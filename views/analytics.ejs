        <div id="view-chart-wrapper">
            <div class="controllers-wrapper">
                <h4 class="text-center">选择时间</h4>
                <div class="datepicker input-group input-daterange">
                    <input type="text" class="form-control" id="view-Chart-Start-Time">
                    <span class="input-group-addon">to</span>
                    <input type="text" class="form-control" id="view-Chart-End-Time">
                </div>
                <div class="checkbox-wrapper view-data-checkbox-wrapper">
                    <div class="controller-label">纵轴数据</div>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="view-checkbox" value="PV"> PV
                    </label>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="view-checkbox" value="UV"> UV
                    </label>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="view-checkbox" value="newstories" checked="checked"> 制作量
                    </label>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="view-checkbox" value="completed" checked="checked"> 已完成
                    </label>
                    <label class="checkbox-inline hidden">
                        <input type="checkbox" name="view-checkbox" value="shared"> 已分享
                    </label>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="view-checkbox" value="newusers" checked="checked"> 新用户
                    </label>
                    <label class="checkbox-inline">
