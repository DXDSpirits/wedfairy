<!DOCTYPE html>

<% include includes/html %>

<head>
    <title>八音盒</title>

    <% include includes/metadata %>

    <link rel="stylesheet" href="<%= static_url %>stylesheets/bootstrap-custom.css?v=<%= build_no %>">

    <link rel="stylesheet" href="<%= tat_static %>stylesheets/font-awesome.css">
    <link rel="stylesheet" href="<%= tat_static %>stylesheets/animate.min.css">
    <link rel="stylesheet" href="<%= tat_static %>stylesheets/amour.css">

    <link rel="stylesheet" href="<%= static_url %>stylesheets/bootstrap-datepicker/bootstrap-datepicker.css?v=<%= build_no %>">
    <link rel="stylesheet" href="<%= static_url %>stylesheets/tat_analytics.css?v=<%= build_no %>">
    <% include includes/stylesheets %>
    <% include includes/capability %>
    <% include includes/production %>

    <script src="<%= tat_static %>javascripts/vendor/modernizr-2.7.1.min.js"></script>
</head>
<body>
    <% include layouts/header %>
    <div class="wedfairy-dashboard clearfix">
        <div id="view-chart-wrapper">
            <div class="controllers-wrapper">
                <h4 class="text-center">选择时间</h4>
                <div class="datepicker input-group input-daterange">
                    <input type="text" class="form-control" id="view-Chart-Start-Time">
                    <span class="input-group-addon">to</span>
                    <input type="text" class="form-control" id="view-Chart-End-Time">
                </div>
                <div class="checkbox-wrapper view-data-checkbox-wrapper">
                    <div class="controller-label">纵轴数据</div>
                    <h5>PV</h5>
                    <div class="data-category-item">
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_all" checked="checked"> all
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_apps"> apps
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_blog"> blog
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_compose"> compose
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_www"> www
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="PV_story"> story
                        </label>
                    </div>
                    <h5>UV</h5>
                    <div class="data-category-item">
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_all" checked="checked"> all
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_apps"> apps
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_blog"> blog
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_compose"> compose
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_www"> www
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="UV_story"> story
                        </label>
                    </div>
                    <h5>Sessions</h5>
                    <div class="data-category-item">
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_all" checked="checked"> all
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_apps"> apps
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_blog"> blog
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_compose"> compose
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_www"> www
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Sessions_story"> story
                        </label>
                    </div>
                    <h5>Users</h5>
                    <div class="data-category-item">
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_all" checked="checked"> all
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_apps"> apps
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_blog"> blog
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_compose"> compose
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_www"> www
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="Users_story"> story
                        </label>
                    </div>
                    <h5>其他</h5>
                    <div class="data-category-item">
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="newstories" checked="checked"> 制作量
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="completed" checked="checked"> 已完成
                        </label>
                        <label class="checkbox-inline hidden">
                            <input type="checkbox" name="view-checkbox" value="shared"> 已分享
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="newusers" checked="checked"> 新用户
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="wishes" checked="checked"> 评论量
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" name="view-checkbox" value="likes" checked="checked"> 点赞量
                        </label>
                    </div>
                </div>
                <div class="radio-wrapper radio">
                    <div class="controller-label">横轴单位</div>
                    <label class="radio-inline">
                        <input type="radio" name="view-radio" value="byYear"> 年
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="view-radio" value="byMonth"> 月
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="view-radio" value="byWeek"> 周
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="view-radio" value="byDay" checked="checked"> 天
                    </label>
                </div>
                <div class="submit-wrapper text-right"><button class="btn btn-primary submit" type="button">Submit</button></div>

            </div>
            <div class="charts-wrapper">
                <!-- 第一个图标 -->

                <div id="chart-time-data" class="wedfairy-charts">

                    <div class="canvas-wraper">
                        <div class="data-label">浏览数据</div>
                        <canvas id="storyDataChart" width="600" height="300">
                        </canvas>
                    </div>
                    <div class="show-legend"></div>
                </div>
                </br>
                <div id="chart-events" class="form-inline">
                    <div class="form-group">
                        <div class="input-group date">
                            <input type="text" class="form-control" id="datepicker-events">
                            <div class="input-group-addon">
                                <span class="glyphicon glyphicon-th"></span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                    <input type="text" id="event-name" class="form-control"  placeholder="写入当天事件">
                    </div>
                    <button type="botton" class="btn btn-success">确定</button>
                </div>
            </div>
        </div>

        <div id="story-chart-wrapper">
            <div class="controllers-wrapper">
                <h4 class="text-center">选择时间</h4>
                <div class="datepicker input-group input-daterange">
                    <input type="text" class="form-control" id="story-Chart-Start-Time">
                    <span class="input-group-addon">to</span>
                    <input type="text" class="form-control" id="story-Chart-End-Time">
                </div>
                <div class="checkbox-wrapper story-values-checkbox-wrapper">
                    <div class="controller-label">纵轴数据</div>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="story-checkbox" value="newstories" checked="checked"> 制作量
                    </label>
                    <label class="checkbox-inline">
                        <input type="checkbox" name="story-checkbox" value="completed" checked="checked"> 已完成
                    </label>
                    <label class="checkbox-inline hidden">
                        <input type="checkbox" name="story-checkbox" value="shared"> 已分享
                    </label>
                    <label class="checkbox-inline hidden">
                        <input type="checkbox" name="story-checkbox" value="wishes"> 评论量
                    </label>
                </div>
                <div class="submit-wrapper text-right"><button class="btn btn-primary submit" type="button">Submit</button></div>
            </div>
            <div class="charts-wrapper">
                <!-- 第二个图标 -->

                <div id="chart-story-data" class="wedfairy-charts">

                    <div class="canvas-wraper">
                        <div class="data-label">故事数据</div>
                        <canvas id="userDataChart" width="600" height="200">
                        </canvas>
                    </div>
                    <div class="show-legend"></div>
                </div>

            </div>
        </div>

        <div id="top-story-table-wrapper">
            <div class="controllers-wrapper">
                <h4 class="text-center">选择时间</h4>
                <div class="datepicker input-group input-daterange">
                    <input type="text" class="form-control" id="top-story-table-Start-Time">
                    <span class="input-group-addon">to</span>
                    <input type="text" class="form-control" id="top-story-table-End-Time">
                </div>

                <div class="submit-wrapper text-right"><button class="btn btn-primary submit" type="button">Submit</button></div>
            </div>
            <div class="charts-wrapper">
                <div class="inline-tb top-likes">
                    <h4 class="text-center">点赞TOP10</h4>
                    <table class="table table-striped table-bordered table-hover table-condensed story-tb">
                        <thead>
                            <tr class="success">
                                <td class="show-short-text">故事</td>
                                <td>name</td>
                                <td>点赞量</td>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div class="inline-tb top-wishes">
                    <h4 class="text-center">评论TOP10</h4>
                    <table class="table table-striped table-bordered table-hover table-condensed story-tb">
                        <thead>
                            <tr class="info">
                                <td class="show-short-text">故事</td>
                                <td>name</td>
                                <td>评论量</td>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div class="inline-tb top-pageviews">
                    <h4 class="text-center">PVTOP10</h4>
                    <table class="table table-striped table-bordered table-hover table-condensed story-tb">
                        <thead>
                            <tr class="danger">
                                <td class="show-short-text">故事</td>
                                <td>name</td>
                                <td>浏览量</td>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
        <div id="keywords-table-wrapper">
            <div class="controllers-wrapper">
                <h4 class="text-center">时间周期</h4>
                <h5 class="text-center">2015/12/25 至今</h5>
            </div>
            <div class="charts-wrapper">
                <div class="keywords-table">
                    <h4 class="text-center">关键词数据</h4>
                    <table class="table table-striped table-bordered table-hover table-condensed">
                        <thead>
                            <tr class="success">
                                <td class="show-short-text">关键词</td>
                                <td>PV</td>
                                <td>UV</td>
                                <td>Sessions</td>
                                <td>转化率</td>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

<% include includes/javascripts %>
<script src="<%= tat_static %>javascripts/vendor/jquery-2.1.1.min.js"></script>
<script src="<%= tat_static %>javascripts/vendor/underscore.min.js"></script>
<script src="<%= tat_static %>javascripts/vendor/handlebars.min.js"></script>
<script src="<%= tat_static %>javascripts/vendor/backbone.min.js"></script>
<script src="<%= tat_static %>javascripts/vendor/bootstrap.min.js"></script>

<script src="<%= tat_static %>javascripts/plugins/fastclick.js"></script>
<script src="<%= tat_static %>javascripts/plugins/plupload.full.min.js"></script>
<script src="<%= tat_static %>javascripts/plugins/qiniu.js"></script>
<script src="<%= tat_static %>javascripts/plugins/moment.js"></script>
<script src="<%= tat_static %>javascripts/plugins/jquery.qrcode.min.js"></script>

<script src="<%= tat_static %>javascripts/amour.js?v=<%= build_no %>"></script>
<script src="<%= static_url %>javascripts/Chartjs/Chart.min.js?v=<%= build_no %>"></script>
<script src="<%= static_url %>javascripts/randomColor/randomColor.js?v=<%= build_no %>"></script>
<script src="<%= static_url %>javascripts/bootstrap-datepicker/bootstrap-datepicker.js?v=<%= build_no %>" charset="UTF-8"></script>
<script src="<%= static_url %>javascripts/tat_analytics.js?v=<%= build_no %>"></script>
<% include includes/analytics %>

</body>
</html>
